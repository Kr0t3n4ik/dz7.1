import operator

operators = {
    "+": operator.add,
    "-": operator.sub,
    "/": operator.mul,
    "//": operator.floordiv,
    "*": operator.mul,
    "**": operator.pow,
    "%": operator.mod,
}
def math_result(first_num, second_num, oprt):
    return operators.get(oprt)(first_num, second_num)
first_num = int(input("Enter first number"))
second_num = int(input("Enter second number"))
oprt = input("Enter operator")
print(math_result(first_num, second_num, oprt))
